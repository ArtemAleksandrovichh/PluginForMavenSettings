
<idea-plugin>
    <id>tnt.evoscada.PluginForMavenSettings</id>

    <name>PluginForMavenSettings</name>

    <vendor email="temikzhavoronkov@mail.ru" url="">ArtemZhavoronkov</vendor>

    <description>Плагин при первом запуске проекта(клонирование с битбакета, создание и тд) настраивает Maven VM options
    </description>

    <depends>com.intellij.modules.platform</depends>
    <depends>org.jetbrains.idea.maven</depends>

    <extensions defaultExtensionNs="com.intellij">
        <postStartupActivity implementation="tnt.evoscada.pluginformavensettings.SetMavenSettingsStartupActivity"/>
    </extensions>
</idea-plugin>